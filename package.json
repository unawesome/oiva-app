{
  "name": "Oiva-app",
  "version": "1.0.0",
  "description": "Oiva-app with Node.js + Express",
  "author": "Smartjuuso",
  "license": null,
  "keywords": [
    "nodejs",
    "express",
    "typescript"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pg-promise": "^10.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "nodemon": "^2.0.1",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "oiva": "tsc-watch --onSuccess \"node dist/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unawesome/oiva-back.git"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts"
  }
}
